[
  {
    "context": "VimControl && !menu",
    "bindings": {}
  },
  {
    "context": "vim_mode == normal && !menu",
    "bindings": {
      "shift-y": [
        "workspace::SendKeystrokes",
        "y $"
      ]
    }
  },
  {
    "context": "vim_mode == insert",
    "bindings": {}
  },
  {
    "context": "EmptyPane || SharedScreen",
    "bindings": {
      "space f": "file_finder::Toggle"
    }
  },
  {
    "context": "Dock",
    "bindings": {
      "ctrl-w h": [
        "workspace::ActivatePaneInDirection",
        "Left"
      ],
      "ctrl-w l": [
        "workspace::ActivatePaneInDirection",
        "Right"
      ],
      "ctrl-w k": [
        "workspace::ActivatePaneInDirection",
        "Up"
      ],
      "ctrl-w j": [
        "workspace::ActivatePaneInDirection",
        "Down"
      ]
    }
  }
]
